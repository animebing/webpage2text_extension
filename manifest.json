{
    "manifest_version": 3,
    "name": "Webpage2Text",
    "version": "1.0.0",
    "description": "Extract text from the current page and copy to clipboard",
    "permissions": ["scripting", "activeTab"],
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "webpage2text"
    }
}